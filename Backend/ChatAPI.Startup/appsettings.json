{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "PersistenceSettings": {
    "ConnectionStringTemplate": "Server={host};Database={database};Username={username};Password={Password};",
    "Host": "localhost",
    "Database": "chatapp",
    "Username" : "postgres",
    "Password" : "postgres",
    
    "AuthenticationSettings" : {
